{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "no-console": "off",
      "indent": "off",
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/jsx-quotes": 0,
      "template-curly-spacing": "off"
    }
}

